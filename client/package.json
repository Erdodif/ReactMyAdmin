{
  "name": "hu.petrik.reactmyadmin.frontend",
  "version": "0.0.1",
  "description": "A react application's frontend targeting the main features of phpMyAdmin",
  "repository": {
    "type": "git",
    "url": "https://github.com/Erdodif/ReactMyAdmin.git"
  },
  "keywords": [
    "react",
    "mysql",
    "xampp"
  ],
  "scripts": {
    "start": "env-cmd -f ../.env react-scripts start",
    "build": "env-cmd -f ../.env react-scripts build",
    "test": "env-cmd -f ../.env react-scripts test",
    "eject": "env-cmd -f ../.env react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "dependencies": {
    "dotenv": "^16.0.0",
    "env-cmd": "^10.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "^5.0.0",
    "sass": "^1.49.9",
    "web-vitals": "^2.1.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
